<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="report_patient">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="patient">
                <t t-call="web.external_layout">
                    <div class="header">
                    
                        <h1 style="text-align: center;">Patient Information</h1>
                    </div>
                    <div class="page">
                        <div class="row">
                            <!-- Main content of the report -->
                            <h2>Patient Information</h2>
                            <p>Name: <span t-field="patient.name_is"/></p>
                            <p>Age: <span t-field="patient.age"/></p>
                            <p>IS Child: <span t-field="patient.is_child"/></p>
                            <p>Age: <span t-field="patient.gender"/></p>
                        </div>
                    </div>
                    <div class="footer">
                    
                        <p>Footer Content Goes Here</p>
                    </div>
                </t>
            </t>
        </t>
    </template>
</odoo>
